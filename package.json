{
  "name": "leveupbackendnode",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "dev":   "nodemon --watch src --ignore public/ --ignore logs/ src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "logs":  "powershell Get-Content -Path ./server-logs/websocket.log -Wait -Tail 10",
    "errors": "Get-Content -Path ./logs/websocket-error.log -Wait -Tail 10"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "typescript": "^5.7.3",
    "winston": "^3.17.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/ws": "^8.5.13",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2"
  }
}
